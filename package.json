{
  "name": "new-biss-ctf",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --proxy-config .\\src\\proxy.conf.json",
    "api:compile": "npx ng-openapi-gen --input ./backend-api.yaml --ignore-unused-models false --output src/app/modules/openapi",
    "build": "npm run api:compile && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "docker:build": "docker build -t biss-ctf-front:latest .",
    "docker:run": "docker run -p 8000:80 -d biss-ctf-front:latest"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/cdk": "^17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/material": "^17.1.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/router": "^17.1.0",
    "angular-material": "^1.1.24",
    "fs": "^0.0.1-security",
    "material": "^0.7.9",
    "ng-openapi-gen": "^0.51.0",
    "ngx-cookie-service": "^17.0.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.0",
    "@angular/cli": "^17.1.0",
    "@angular/compiler-cli": "^17.1.0",
    "@types/jasmine": "~5.1.4",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.3.3"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
